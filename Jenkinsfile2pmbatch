node{
echo "Job Name is:  ${env.JOB_NAME}"
echo "Node Name is:  ${env.NODE_NAME}"
def mavenHome = tool name: 'maven3.9.7'
//Get the Code from the GitHub Server
stage('CheckoutCode') {
git branch: 'development', credentialsId: '4b8bb8c3-9d5f-4ac7-b407-e634233e7e95', url: 'https://github.com/wst-devteam/maven-web-application.git'
}
//Do the Build by using Maven Build Tool
stage('Build'){
//To get the Variable Value
sh "${mavenHome}/bin/mvn clean package"	
}
